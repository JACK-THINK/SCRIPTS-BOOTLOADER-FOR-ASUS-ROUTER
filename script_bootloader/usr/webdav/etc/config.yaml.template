# Server related settings
address: 0.0.0.0
port: 35241
auth: true
tls: false
cert: cert.pem
key: key.pem
prefix: /

# Default user settings (will be merged)
scope: /root
modify: true
rules: []

# CORS configuration
cors:
  enabled: false
  credentials: true
  allowed_headers:
    - Depth
  allowed_hosts:
    - http://localhost:8080
  allowed_methods:
    - GET
  exposed_headers:
    - Content-Length
    - Content-Range

users:
  - username: ADMINUSER
    password: ADMINPASSWORD
    scope: /root