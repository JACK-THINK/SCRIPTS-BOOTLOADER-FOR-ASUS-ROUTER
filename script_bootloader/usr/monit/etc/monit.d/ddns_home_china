check program ddns_home_china with path "/opt/script_bootloader/usr/ddns/bin/ddns_home_china_enable.service" every 20 cycles
    start program = "/opt/script_bootloader/usr/ddns/bin/ddns_home_china_enable.service"
    stop program = "/opt/script_bootloader/usr/ddns/bin/ddns_home_china_disable.service"
    if status == 1 then exec "/opt/script_bootloader/usr/ddns/bin/ddns_home_china_disable.service"